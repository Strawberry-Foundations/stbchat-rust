language: en_US
update_channel: "stable"
detect_same_system_messages: true
experimental_debug_mode: false
extreme_debug_mode: false
config_ver: 7

ui:
  message_format: gray
  enable_terminal_bell: true

notification:
  enabled: true
  use_legacy_notifier: false
  icon_path: "/path/to/your/icon/sf_logo_small.ico"

networking:
  online_mode: true
  keep_alive: true
  latency_mode: false
  latency_mode_time: 1
  recv_allowed_bytes: 8192

autoserver:
  enabled: false
  server_id: 0

server:
  0: 
    name: strawberryfoundations.org
    address: 45.131.109.170
    port: 52800
    type: Main
    autologin: false
    compatibility_mode: false # Change to true if you want to connect to servers with v1.8.1 or below
    credentials:
      username: yourname
      password: yourpassword # Warning: This is very insecure
  1: 
    name: localhost
    address: localhost
    port: 8080
    type: Off Topic
  2: 
    name: your.chatserver.com
    address: 0.0.0.0
    port: 8080
    type: Some Type