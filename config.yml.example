language: en_US
update_channel: "stable"
config_ver: 8

ui:
  message_format: gray
  enable_terminal_bell: true
  serverlist_show_type: false
  serverlist_show_address: false

notification:
  enabled: true
  use_legacy_notifier: false
  icon_path: "/path/to/your/icon/sf_logo_small.ico"

networking:
  online_mode: true

autoserver:
  enabled: false
  server_id: 0

server:
  0: 
    name: strawberryfoundations.org
    address: 45.131.109.170
    port: 52800
    type: Main
    autologin: false
    compatibility_mode: false # Change to true if you want to connect to servers with v1.8.1 or below
    credentials:
      username: yourname
      password: yourpassword # Warning: This is very insecure
  1: 
    name: localhost
    address: localhost
    port: 8080
    type: Off Topic
  2: 
    name: your.chatserver.com
    address: 0.0.0.0
    port: 8080
    type: Some Type